{
  "name": "schools-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "author": "Schools App",
  "license": "MIT",
  "homepage": "https://github.com/chevck/school-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/chevck/school-app.git"
  },
  "description": "Schools App",
  "scripts": {
    "dev": "vite",
    "dev--electron": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "start": "vite",
    "build": "vite build",
    "build--electron": "vite build && electron-builder",
    "build:dmg--electron": "npm run build && ./fix-dmg.sh",
    "build:win--electron": "vite build && ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ CSC_IDENTITY_AUTO_DISCOVERY=false npx electron-builder --win --x64",
    "build:all--electron": "npm run build:dmg && npm run build:win",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "build": {
    "buildDependenciesFromSource": true,
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "appId": "com.schoolsapp.app",
    "files": [
      "dist",
      "electron"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "identity": null,
      "entitlements": null,
      "entitlementsInherit": null,
      "gatekeeperAssess": false
    },
    "win": {
      "target": "portable",
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.9.0",
    "concurrently": "^9.2.0",
    "formik": "^2.4.6",
    "html-react-parser": "^5.2.5",
    "html2pdf.js": "^0.9.0",
    "iconsax-reactjs": "^0.0.8",
    "lucide-react": "^0.511.0",
    "moment": "^2.30.1",
    "motion": "^12.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-loading-skeleton": "^3.5.0",
    "react-quill-new": "^3.4.6",
    "react-router-dom": "^7.6.0",
    "react-select": "^5.10.1",
    "sonner": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "vite-plugin-electron": "^0.29.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.24",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "daisyui": "^5.0.37",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^8.0.4"
  }
}
